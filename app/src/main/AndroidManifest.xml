<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shardprf">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application_class.ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShardPrf"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.example.StarterActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.search_with_edit_textview.HomeSearchActivity"
            android:exported="false"/>
        <activity
            android:name="com.example.search_with_searchview.DetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.search_with_searchview.SearchActivity"
            android:exported="false"/>
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>